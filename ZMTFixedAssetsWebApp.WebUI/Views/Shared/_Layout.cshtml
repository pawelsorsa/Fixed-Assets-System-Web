<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    @if (false) {
        <script src="../../Scripts/jquery-1.5.1-vsdoc.js" type="text/javascript"></script>
    }
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/MyScripts/personSearch.js")" type="text/javascript"></script>  
    <script src="@Url.Content("~/Scripts/MyScripts/jquery.livequery.js")" type="text/javascript"></script>  
    <script src="@Url.Content("~/Scripts/MyScripts/Myhistory.js")" type="text/javascript"></script>  
    <script src="@Url.Content("~/Scripts/MyScripts/jquery.history.js")" type="text/javascript"></script>
</head>


<body>
     <div id="wrapper">
	<div id="header">
		<h1>PLk ZMT</h1>
		<h2>SYSTEM DO ZARZĄDZANIE </h2>

	</div>
    <div id="menu">
		<ul id="menuu">
            <li>@Html.ActionLink("Home", "Index", "Person", null, null)</li>
           @* <li>@Html.Raw(@HttpUtility.UrlDecode(@Html.ActionLink("About", "List", "Person").ToHtmlString())))</li> *@
            <li>@Html.ActionLink("About", "List", "Person")</li>
            <li>@Ajax.ActionLink("Powrót", "Index", "Person", null, new AjaxOptions { UpdateTargetId = "ajax_main_content", HttpMethod = "GET", OnFailure = "OnFailure" }, new { @onclick = "raiseCounter()", @rel = "ajax" })</li>

        </ul>
		
		
	</div>

	<div id="content">
		<div id="right">
			<div class="post">
				@RenderBody()
			</div>
		</div>
	</div>

	<div id="footer">
		<p class="copyright">Copyright &copy; 2012 Paweł Sorsa. Template Designed by <a href="http://www.pawelsorsa.com/">pawelsorsa.com</a></p>
		<p class="links"><a href="http://validator.w3.org/check/referer" class="xhtml">Valid XHTML</a>&nbsp;&nbsp;&nbsp;<a href="http://jigsaw.w3.org/css-validator/check/referer" class="css">Valid CSS</a> </p>
	</div
</div>
</body>
</html>
